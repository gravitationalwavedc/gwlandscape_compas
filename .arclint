{
  "exclude": "(.*/migrations/)",
  "linters": {
    "pyflakes": {
      "type": "pyflakes",
      "include": "(\\.py$)",
      "exclude": "(^react/|^venv/)"
    },
    "flake8": {
      "type": "flake8",
      "include": "(\\.py$)",
      "exclude": "(^react/|^venv/)",
      "flags": [
         "--max-line-length=120"
      ]
    }
  }
}
